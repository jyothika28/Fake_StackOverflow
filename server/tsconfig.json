{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "inlineSourceMap": true,
    "typeRoots": ["node_modules/@types"]
  },
 "include": ["*.ts"],
  "exclude": [
    "node_modules",
    "dist"
  ]
}